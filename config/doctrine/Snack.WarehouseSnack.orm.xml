<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd"
>
    <entity name="Tab\Domain\Model\Snack\WarehouseSnack" table="warehouse_snacks">
        <id name="snack" type="integer" association-key="true" />
        <field name="quantity" type="integer" />

        <one-to-one
                field="snack"
                target-entity="Tab\Domain\Model\Snack\Snack"
                inversed-by="warehouseSnack"
        >
            <join-column name="snack_id" referenced-column-name="snack_id" />
        </one-to-one>
    </entity>
</doctrine-mapping>
